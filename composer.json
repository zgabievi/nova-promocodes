{
    "name": "zgabievi/nova-promocodes",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "zgabievi/laravel-promocodes": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zorb\\NovaPromocodes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zorb\\NovaPromocodes\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
